{
    "elements" : {
        "appsrc" : {
            "property" : {
                "is-live" : true,
                "format" : 3
            }
        },
        "videoscale" : {
            "caps" : "video/x-raw,width=1280,height=720"
        },
        "x264enc" : {
            "property" : {
                "name" : "x264enc",
                "bframes" : 3,
                "threads" : 0,
                "qp-min" : 1,
                "qp-max" : 51,
                "qp-step" : 10,
                "vbv-buf-capacity" : 10000,
                "bitrate" : 1500,
                "byte-stream" : "TRUE",
                "rc-lookahead" : 200,
                "pass" : 0,
                "mb-tree" : true,
                "dct8x8" : true,
                "speed-preset" : 0,
                "option-string" : ":ref=3:me=hex:subme=8:merange=24:nf=1:deblock=1,-2:weightp=1:b-pyramid=2:direct=spatial:nal-hrd=vbr"
            }
        },
        "voaacenc" : {
            "property" : {
                "bitrate" : 64000
            }
        },
        "hlssink" : {
            "property" : {
                "playlist-location" : "",
                "location" : "",
                "max-files" : 9999,
                "playlist-length" : 9999,
                "target-duration" : 10
            }
        }
    },
    "bins" : [
        "appsrc name=video ! queue ! videoconvert ! queue ! videoscale ! queue ! x264enc ! queue ! muxer.",
        "appsrc name=audio ! queue ! audioconvert ! voaacenc ! queue ! aacparse ! queue ! muxer.",
        "mpegtsmux name=muxer ! queue ! hlssink"
    ]
}

