{
    "elements": {
        "appsrc": {
            "property": {
                "is-live": true,
                "format": 3
            }
        },
        "videoscale" : {
            "caps" : "video/x-raw,width=640,height=360"
        },
        "x264enc": {
            "property": {
                "bframes" : 3,
                "threads" : 4,
                "qp-min" : 1,
                "qp-max" : 51,
                "qp-step" : 10,
                "bitrate" : 300,
                "byte-stream" : true,
                "pass" : 0,
                "mb-tree" : true,
                "option-string" : ":ref=3:me=umh:subme=16:merange=64:nf=1:deblock=1,-2:weightp=1:b-pyramid=2:direct=spatial:nal-hrd=vbr"
            }
        },
        "voaacenc": {
            "property": {
                "bitrate": 64000
            }
        },
        "appsink": {
            "property": {
                "sync": false
            }
        }
    },
    "bins": [
        "appsrc name=video ! queue ! videoconvert ! queue ! videoscale ! queue ! x264enc ! queue ! muxer.",
        "appsrc name=audio ! queue ! audioconvert ! voaacenc ! muxer.",
        "mpegtsmux name=muxer ! queue ! appsink"
    ],
    "udpstreaming": "225.0.0.1:10000"
}
