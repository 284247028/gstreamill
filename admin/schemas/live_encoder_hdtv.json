{
    "elements": {
        "appsrc": {
            "property": {
                "is-live": true,
                "format": 3
            }
        },
        "videoscale" : {
            "caps" : "video/x-raw,width=1280,height=720"
        },
        "x264enc" : {
            "property" : {
                "name" : "x264enc",
                "bframes" : 3,
                "threads" : 8,
                "qp-min" : 1,
                "qp-max" : 51,
                "qp-step" : 10,
                "vbv-buf-capacity" : 10000,
                "bitrate" : 1500,
                "byte-stream" : "TRUE",
                "rc-lookahead" : 250,
                "pass" : 0,
                "mb-tree" : true,
                "dct8x8" : true,
                "speed-preset" : 0,
                "option-string" : ":ref=3:me=umh:subme=16:merange=64:nf=1:deblock=1,-2:weightp=1:b-pyramid=2:direct=spatial:nal-hrd=vbr"
            }
        },
        "voaacenc": {
            "property": {
                "bitrate": 64000
            }
        },
        "appsink": {
            "property": {
                "sync": false
            }
        }
    },
    "bins": [
        "appsrc name=video ! queue ! videoconvert ! queue ! videoscale ! queue ! x264enc ! queue ! muxer.",
        "appsrc name=audio ! queue ! audioconvert ! queue ! voaacenc ! muxer.",
        "mpegtsmux name=muxer ! queue ! appsink"
    ],
    "udpstreaming": "225.0.0.1:10000"
}
